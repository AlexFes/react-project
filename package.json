{
  "name": "react-project",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "8.9.0",
    "npm": "5.8.0"
  },
  "scripts": {
    "start": "node bin/www",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "client": "npm start --prefix client",
    "server": "nodemon bin/www",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "~2.6.9",
    "ejs": "^2.5.7",
    "express": "~4.15.5",
    "mongoose": "^5.0.14",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.3",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "stripe": "^6.0.0"
  }
}
